{
  "name": "kensobi-spchistogram-panel",
  "version": "1.4.0",
  "description": "Histogram panel with SPC control limits, bell curves, and capability indices.",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "e2e": "npx playwright install && npx @grafana/plugin-e2e run",
    "e2e:update": "npx playwright install && npx @grafana/plugin-e2e run --update-screenshots",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "author": "Kenso Software",
  "license": "AGPL-3.0-only",
  "devDependencies": {
    "@babel/core": "7.29.0",
    "@grafana/eslint-config": "^9.0.0",
    "@grafana/plugin-e2e": "3.3.0",
    "@grafana/tsconfig": "^2.0.1",
    "@stylistic/eslint-plugin-ts": "^4.4.1",
    "@swc/core": "1.15.11",
    "@swc/helpers": "0.5.18",
    "@swc/jest": "^0.2.39",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@types/jest": "^30.0.0",
    "@types/lodash": "4.17.23",
    "@types/node": "20.17.15",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "copy-webpack-plugin": "13.0.1",
    "css-loader": "7.1.3",
    "eslint": "9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsdoc": "^62.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-webpack-plugin": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "9.1.0",
    "glob": "13.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "prettier": "3.8.1",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.97.3",
    "sass-loader": "16.0.7",
    "style-loader": "4.0.0",
    "swc-loader": "^0.2.7",
    "ts-node": "10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.9.3",
    "webpack": "5.105.2",
    "webpack-cli": "6.0.1",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">= 20 <25"
  },
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@grafana/data": "^11.6.10",
    "@grafana/runtime": "^11.6.10",
    "@grafana/ui": "^11.6.10",
    "ml-levenberg-marquardt": "4.1.3",
    "tinycolor2": "^1.6.0",
    "tslib": "^2.8.1"
  },
  "peerDependencies": {
    "react": "^18.2.0 || ^19.0.0",
    "react-dom": "^18.2.0 || ^19.0.0"
  },
  "resolutions": {
    "cross-spawn": "^7.0.6",
