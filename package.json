    "@grafana/tsconfig": "^1.3.0-rc1",
    "@swc/core": "1.4.2",
    "@swc/helpers": "0.5.11",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "15.0.2",
    "@types/jest": "^29.5.12",
    "@types/lodash": "4.17.4",
    "@types/node": "20.14.2",
